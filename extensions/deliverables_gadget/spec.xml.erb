<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="HTK Deliverables v<%= gadget_config['version']%>"
    description=""
    height="200"
    scrolling="true"
    author="Tim Harrison"
    author_email="tharrison@lxdinc.com"
    author_location="Raleigh, NC">

    <Require feature="dynamic-height"/>
    <Require feature="google.contentmatch">
      <Param name="extractors">
        google.com:MessageIDExtractor,google.com:EmailTimeExtractor,google.com:SenderEmailExtractor,google.com:RecipientToEmailExtractor,google.com:RecipientCCEmailExtractor,google.com:SubjectExtractor,google.com:EmailBodyExtractor
      </Param>
    </Require>
  </ModulePrefs>

  <Content type="html" view="card">
    <![CDATA[
      <style>
        <%= insert_file Rails.root.join('public', 'assets', 'deliverables_gadget.css') %>
      </style>
      <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
      <script type="text/javascript">
        <%= insert_file Rails.root.join('public', 'assets', 'deliverables_chrome.js') %>
        $(function() {
          deliverablesGadgetReady();
        });
      </script>
    ]]>
  </Content>
</Module>