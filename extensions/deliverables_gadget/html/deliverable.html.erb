{{#with deliverable}}
<div class="htk-show" id="htk-detail-{{id}}" data-id="{{id}}">
		<p>{{title}}<br />
			Created by: {{owner.first_name}}<br />
			{{#if responsible}}
			Assigned to: {{responsible.first_name}}
			{{/if}}
		</p>
		{{#if parent}}
		<p>Parent is {{parent.title}}</p>
		{{/if}}
		{{simpleFormat description}}
		<ul class="htk-tool">
			<li class="htk-edit"><%= image_tag 'edit.png' %></li>
			{{#if completed_by_id}}
			<li class="htk-action htk-complete"><%= image_tag 'green-check.png' %></li>
			{{else}}
			<li class="htk-action htk-complete"><%= image_tag 'check.png' %></li>
			{{/if}}
			{{#unless parent}}
			<li class="htk-action htk-disassociate"><%= image_tag 'cross.png' %></li>
			{{/unless}}
		</ul>
</div>
{{/with}}
