function DeliverableUser(attributes) {
	HtkModel.call(this, attributes);
}

// DeliverableUser.prototype.user = null;

DeliverableUser.prototype = Object.create(HtkModel.prototype, {
	api_url : {	value : function() { return "/api/v1/deliverables/" + this.deliverable_id + "/users"; } },
	type_key : { value : "deliverable_user" },
	attribute_keys : { value : <%= DeliverableUser.columns.map(&:name).to_json %> },
	registry_hook : { 
		value : function() {
			this.user = User.prototype.find_cached(this.user_id);
		}
	}
});

DeliverableUser.prototype.edit_access_id = "<%= DeliverableAccess.edit.id %>";
