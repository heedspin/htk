function ThreadDeliverable(attributes) {
	HtkModel.call(this, attributes);
}

ThreadDeliverable.prototype = Object.create(HtkModel.prototype, {
	api_url : {	value : function() { return "/api/v1/thread_deliverables"; } },
	single_key : { value : "thread_deliverables" },
	collection_key : { value : "thread_deliverables" },
	attribute_keys : { value : <%= ThreadDeliverable.columns.map(&:name).to_json %> },	
});

ThreadDeliverable.prototype.destroy = function(query_params, callbacks) {
  htkRequest("DELETE", this.api_url() + "/byparam", query_params, function(obj) {
    var rc = "";
    if (obj.rc) rc = obj.rc;
    if (rc == 200) {
      htkLog("Delete thread deliverable succeeded");
	    if (callbacks.success && (typeof(callbacks.success) === "function")) callbacks.success(obj);
    } else {
      htkLog("Deleting thread deliverable failed");
	    if (callbacks.error && (typeof(callbacks.error) === "function")) callbacks.error(obj);
    }
  });	
}
