function getContentMatches() {
	var result = [];
	$("input.contentMatch").each(function(index) {
		var obj = new Object();
		element = $(this);
		obj[element.attr("name")] = element.val();
		result.push(obj);
	})
	$("input.multiContentMatch").each(function(index) {
		element = $(this);
		var values = element.val().split(",");
		for (var i=0,max=values.length; i < max; ++i) {
			var obj = new Object();
			obj[element.attr("name")] = values[i];
			result.push(obj);
		}
	})
	htkLog("getContentMatches: " + JSON.stringify(result));
	return result;
}
