function Message(attributes) {
	HtkModel.call(this, attributes);
}

Message.prototype = Object.create(HtkModel.prototype, {
	api_url : {	value : function() { return "/api/v1/messages"; } },
	type_key : { value : "message" },
	attribute_keys : { value : <%= Message.columns.map(&:name).to_json %> }
});
